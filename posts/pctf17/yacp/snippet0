pt = 'A' * (127 * 16)

key = '\x00' * 32
IV = '\x00' * 16
ct = AES.new(key=key, IV=IV, mode=AES.MODE_CBC).encrypt(pt)
c126 = ct[-16:]

c128 = pack(size0, word_size=128)
x128 = AES.new(key=key).decrypt(c128)

padding = '\x10' * 16
c127 = xor(padding, x128)
x127 = AES.new(key=key).decrypt(c127)

p127 = xor(x127, c126)

pt += p127
